    "scripts": {
        "start": "yarn webpack serve --config webpack.dev.js",
        "build": "yarn webpack --config webpack.prod.js",
        "msbuild": "npm run build && node scripts/msbuild.js",
        "build-dev": "yarn webpack --config webpack.dev.js",
        "build-github": "yarn webpack --config webpack.github.js",
        "lint": "eslint ./src --max-warnings=0",
        "lint-fix": "eslint --fix ./src --max-warnings=0",
        "test": "yarn run lint && yarn run build",
        "postinstall": "yarn run generate-api",
        "generate-api": "yarn run patch && node scripts/api_build.js",
        "patch": "yarn patch-package"
    },
    "tgs_api": {
        "type": "version",
        "value": "9.3.0"
    },
    "tgs_api_example_version": {
        "desc": "Fetches a tgs api version from github using the tag name",
        "type": "version",
        "value": "7.3.2"
    },
    "tgs_api_example_url": {
        "desc": "Fetches a swagger json from a remote url",
        "type": "url",
        "value": "https://example.org/swagger.json"
    },
    "tgs_api_example_file": {
        "desc": "Fetches a swagger json from a local file. Can be absolute or relative to the package.json file.",
        "type": "file",
        "value": "src/custom_swagger.json"
    },
    "files": [
        "build/lib",
